{
  "name": "pluto-framework",
  "version": "1.0.0",
  "description": "Framework for Developing with HTML, JS, and SASS",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass --include-path scss sass/main.scss > css/main.css",
    "minify-css": "cleancss css/main.css -o css/main.min.css",
    "dev": "nodemon -e scss -x \"npm run build-css && npm run minify-css\""
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/errepar_sa1/syserrepar/applications/erreparplus/HomePage/noticias-errepar-plus-dev"
  },
  "keywords": [
    "html",
    "css",
    "js",
    "sass",
    "framework",
    "node",
    "nodemon",
    "cleancss",
    "node-sass"
  ],
  "author": "Santiago Pintus",
  "license": "ISC",
  "homepage": "/",
  "devDependencies": {
    "clean-css-cli": "^5.6.0",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.16"
  }
}
